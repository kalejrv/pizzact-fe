{
	"name": "pizzact-fe",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"preview": "vite preview",
		"dev": "vite",
		"css": "tailwindcss -i ./src/tailwind.css -o ./src/styles.css --watch",
		"eslint": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
		"prettier": "prettier --write .",
		"start": "concurrently \"npm run dev\" \"npm run css\"",
		"format": "concurrently \"npm run eslint\" \"npm run prettier\""
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.2.1",
		"react-router-dom": "^6.23.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.4.0",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.34.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"globals": "^15.4.0",
		"prettier": "^3.3.1",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.2.2",
		"typescript-eslint": "^7.12.0",
		"vite": "^5.0.8"
	}
}
